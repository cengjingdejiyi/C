//通过函数调用，计算两个数的最大公约数。
#include<stdio.h>
int A(int a,int b){             //i从1开始循环，判断i是否为 
	int c;                      //a和b的公约数，并赋值给c， 
	for(int i=1;i<=a&&i<=b;i++){//从小到大一直循环，c的值 
		if(b%i==0&&a%i==0){     //一直更新，循环结束c即为 
		c = i;                  //最大公约数 
		}
	}
	printf("%d",c);
	return 0;
}
int main(){
    int x,y;
    scanf("%d,%d",&x,&y);
	A(x,y);
	return 0;
}
//结果：50，100  
//      50
