//通过函数调用，判定一个数是否为完全平方数。
#include<stdio.h>
int A(int i){           //判断一个数是否为完全平方数的
                        //函数，如果是，返回1，结果为真 
	int a=0;            //，如果不是，返回0，结果为假 
	for(a=1;a*a<=i;a++){ 
		if(a*a==i){
			return 1;
		}
//		else{        //如果写上else循环，从 
//			return 0;//2开始循环可能就为假 
//		}            //直接返回0判断该数不是完全平方数 
	}
	return 0;
}
int main(){
	int a;
	scanf("%d",&a);
	if(A(a)){
		printf("该数是一个完全平方数");
	} 
	else{
		printf("该数不是一个完全平方数"); 
	} 
	return 0;
}
//结果：100
// 该数是一个完全平方数 
